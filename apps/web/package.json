{
  "name": "@swim-hub/web",
  "version": "1.0.0",
  "private": true,
  "description": "水泳選手マネジメントシステム - Webアプリケーション",
  "scripts": {
    "dev": "dotenvx run -f .env.local -- next dev",
    "dev:https": "dotenvx run -f .env.local -- node server.js",
    "build": "dotenvx run -f .env.local -- next build",
    "start": "dotenvx run -f .env.local -- next start",
    "preview": "dotenvx decrypt -f .env.development --stdout > .dev.vars && opennextjs-cloudflare build && opennextjs-cloudflare preview",
    "deploy": "opennextjs-cloudflare build && opennextjs-cloudflare deploy",
    "deploy:preview": "opennextjs-cloudflare build && wrangler deploy --env preview",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf .next dist",
    "test": "dotenvx run -f .env.local -- vitest run",
    "test:watch": "dotenvx run -f .env.local -- vitest",
    "test:ui": "dotenvx run -f .env.local -- vitest --ui",
    "test:coverage": "dotenvx run -f .env.local -- vitest run --coverage",
    "test:e2e": "dotenvx run -f .env.local -- playwright test --config e2e/src/config/playwright.config.ts",
    "test:e2e:ui": "dotenvx run -f .env.local -- playwright test --config e2e/src/config/playwright.config.ts --ui",
    "test:e2e:headed": "dotenvx run -f .env.local -- playwright test --config e2e/src/config/playwright.config.ts --headed",
    "test:e2e:debug": "dotenvx run -f .env.local -- playwright test --config e2e/src/config/playwright.config.ts --debug",
    "test:e2e:codegen": "dotenvx run -f .env.local -- playwright codegen localhost:3000 --config e2e/src/config/playwright.config.ts",
    "test:web": "dotenvx run -f .env.local -- npx playwright test --config e2e/src/config/playwright.config.ts",
    "test:web:local": "E2E_SUPABASE_MODE=local dotenvx run -f .env.local -- npx playwright test --config e2e/src/config/playwright.config.ts",
    "test:web:remote": "E2E_SUPABASE_MODE=remote dotenvx run -f .env.development -- npx playwright test --config e2e/src/config/playwright.config.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.975.0",
    "@dotenvx/dotenvx": "^1.51.1",
    "@heroicons/react": "^2.2.0",
    "@supabase/auth-ui-react": "^0.4.7",
    "@supabase/auth-ui-shared": "^0.1.8",
    "@supabase/ssr": "^0.8.0",
    "@supabase/supabase-js": "^2.80.0",
    "@swim-hub/shared": "*",
    "@tailwindcss/postcss": "^4.1.17",
    "@tanstack/react-query": "^5.90.10",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "exceljs": "^4.4.0",
    "html-to-image": "^1.11.13",
    "next": "^15.5.9",
    "react": "19.2.1",
    "react-device-frameset": "^1.3.4",
    "react-dom": "19.2.1",
    "react-easy-crop": "^5.5.3",
    "recharts": "^3.5.1",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@next/bundle-analyzer": "^15.5.9",
    "@opennextjs/cloudflare": "^1.15.1",
    "@playwright/test": "^1.56.1",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.9",
    "@tailwindcss/oxide": "^4.1.18",
    "@tailwindcss/typography": "^0.5.19",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^25",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitejs/plugin-react": "^5.1.0",
    "@vitest/coverage-v8": "^4.0.8",
    "autoprefixer": "^10.4.21",
    "dotenv": "^17.2.3",
    "eslint": "^9.37.0",
    "eslint-config-next": "^15.5.9",
    "jsdom": "^27.1.0",
    "lightningcss": "^1.30.2",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.16",
    "typescript": "^5",
    "vitest": "^4.0.3",
    "wrangler": "^4.60.0"
  }
}
