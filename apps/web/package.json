{
  "name": "@swim-hub/web",
  "version": "1.0.0",
  "private": true,
  "description": "水泳選手マネジメントシステム - Webアプリケーション",
  "scripts": {
    "dev": "npx dotenvx run -f .env.development -- next dev",
    "build": "npx dotenvx run -f .env.production -- next build",
    "build:vercel": "next build",
    "start": "npx dotenvx run -f .env.production -- next start",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf .next dist",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test --config e2e/src/config/playwright.config.ts",
    "test:e2e:ui": "playwright test --config e2e/src/config/playwright.config.ts --ui",
    "test:e2e:headed": "playwright test --config e2e/src/config/playwright.config.ts --headed",
    "test:e2e:debug": "playwright test --config e2e/src/config/playwright.config.ts --debug",
    "test:e2e:codegen": "playwright codegen localhost:3000 --config e2e/src/config/playwright.config.ts",
    "test:web": "npx playwright test --config e2e/src/config/playwright.config.ts",
    "test:web:local": "E2E_SUPABASE_MODE=local npx playwright test --config e2e/src/config/playwright.config.ts",
    "test:web:remote": "E2E_SUPABASE_MODE=remote npx playwright test --config e2e/src/config/playwright.config.ts"
  },
  "dependencies": {
    "@dotenvx/dotenvx": "^1.51.1",
    "@heroicons/react": "^2.2.0",
    "@supabase/auth-helpers-nextjs": "^0.15.0",
    "@supabase/ssr": "^0.8.0",
    "@supabase/supabase-js": "^2.80.0",
    "@swim-hub/shared": "*",
    "@tailwindcss/postcss": "^4.1.17",
    "@tanstack/react-query": "^5.90.10",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "exceljs": "^4.4.0",
    "next": "^16.0.10",
    "react": "19.2.1",
    "react-device-frameset": "^1.3.4",
    "react-dom": "19.2.1",
    "react-easy-crop": "^5.5.3",
    "recharts": "^3.5.1",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@playwright/test": "^1.56.1",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.9",
    "@tailwindcss/typography": "^0.5.19",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^25",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitejs/plugin-react": "^5.1.0",
    "@vitest/coverage-v8": "^4.0.8",
    "autoprefixer": "^10.4.21",
    "dotenv": "^16.4.7",
    "eslint": "^9.37.0",
    "eslint-config-next": "^16.0.7",
    "jsdom": "^27.1.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.16",
    "typescript": "^5",
    "vitest": "^4.0.3"
  }
}
