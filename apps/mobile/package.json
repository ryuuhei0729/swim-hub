{
  "name": "@swim-hub/mobile",
  "version": "1.0.0",
  "private": true,
  "description": "水泳選手マネジメントシステム - モバイルアプリケーション",
  "main": "index.ts",
  "scripts": {
    "start": "npx dotenvx run -f .env.development -- expo start",
    "android": "npx dotenvx run -f .env.development -- expo start --android",
    "ios": "npx dotenvx run -f .env.development -- expo start --ios",
    "web": "npx dotenvx run -f .env.development -- expo start --web",
    "lint": "eslint .",
    "type-check": "tsc --noEmit",
    "clean": "npx expo clean",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@dotenvx/dotenvx": "^1.51.1",
    "@react-native-async-storage/async-storage": "^2.1.0",
    "@react-native-community/netinfo": "^11.4.1",
    "@react-navigation/bottom-tabs": "^7.1.0",
    "@react-navigation/native": "^7.0.0",
    "@react-navigation/native-stack": "^7.1.0",
    "@supabase/supabase-js": "^2.80.0",
    "@swim-hub/shared": "*",
    "@tanstack/react-query": "^5.90.10",
    "date-fns": "^4.1.0",
    "expo": "~54.0.27",
    "expo-status-bar": "~3.0.9",
    "react": "19.2.0",
    "react-native": "0.81.5",
    "react-native-safe-area-context": "^5.6.2",
    "react-native-screens": "^4.18.0",
    "react-native-web": "^0.21.0",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/js": "^9",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/react": "^16.1.0",
    "@testing-library/react-native": "^13.3.3",
    "@types/react": "~19.2.0",
    "@vitejs/plugin-react": "^5.1.2",
    "@vitest/coverage-v8": "^2.1.8",
    "eslint": "^9.37.0",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.1.0",
    "react-test-renderer": "^19.2.0",
    "typescript": "~5.9.2",
    "typescript-eslint": "^8.18.2",
    "vitest": "^2.1.8"
  }
}
