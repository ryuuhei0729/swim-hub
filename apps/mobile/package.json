{
  "name": "@swim-hub/mobile",
  "version": "1.0.0",
  "private": true,
  "description": "水泳選手マネジメントシステム - モバイルアプリケーション",
  "main": "index.ts",
  "scripts": {
    "start": "npx dotenvx run -f .env.local -- expo start",
    "start:clear": "npx dotenvx run -f .env.local -- expo start --clear",
    "start:local": "npx dotenvx run -f .env.local -- expo start",
    "start:dev": "npx dotenvx run -f .env.development -- expo start",
    "start:prod": "npx dotenvx run -f .env.production -- expo start",
    "android": "npx dotenvx run -f .env.local -- expo start --android",
    "android:clear": "npx dotenvx run -f .env.local -- expo start --android --clear",
    "android:local": "npx dotenvx run -f .env.local -- expo start --android",
    "android:dev": "npx dotenvx run -f .env.development -- expo start --android",
    "android:prod": "npx dotenvx run -f .env.production -- expo start --android",
    "ios": "npx dotenvx run -f .env.local -- expo start --ios --dev-client",
    "ios:run": "npx dotenvx run -f .env.local -- expo run:ios",
    "ios:run:sim": "npx dotenvx run -f .env.local -- expo run:ios -d \"iPhone 16\"",
    "ios:clear": "npx dotenvx run -f .env.local -- expo start --ios --dev-client --clear",
    "ios:local": "npx dotenvx run -f .env.local -- expo start --ios --dev-client",
    "ios:dev": "npx dotenvx run -f .env.development -- expo start --ios --dev-client",
    "ios:prod": "npx dotenvx run -f .env.production -- expo start --ios --dev-client",
    "web": "npx dotenvx run -f .env.local -- expo start --web",
    "lint": "eslint .",
    "type-check": "tsc --noEmit",
    "clean": "npx expo clean",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "submit:ios": "set -a && source .env && set +a && export APPLE_ID APPLE_APP_SPECIFIC_PASSWORD && eas submit --platform ios --latest"
  },
  "dependencies": {
    "@dotenvx/dotenvx": "^1.51.1",
    "@react-native-async-storage/async-storage": "^2.1.0",
    "@react-native-community/netinfo": "^11.4.1",
    "@react-native-picker/picker": "^2.11.1",
    "@react-navigation/bottom-tabs": "7.10.1",
    "@react-navigation/native": "7.1.28",
    "@react-navigation/native-stack": "7.11.0",
    "@supabase/supabase-js": "^2.80.0",
    "@swim-hub/shared": "*",
    "@tanstack/react-query": "^5.90.10",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "expo": "~54.0.33",
    "expo-apple-authentication": "~8.0.8",
    "expo-auth-session": "~7.0.10",
    "expo-calendar": "~15.0.8",
    "expo-clipboard": "8.0.8",
    "expo-crypto": "~15.0.8",
    "expo-dev-client": "^6.0.20",
    "expo-image-manipulator": "~14.0.8",
    "expo-image-picker": "~17.0.10",
    "expo-linking": "~8.0.11",
    "expo-status-bar": "~3.0.9",
    "expo-web-browser": "~15.0.10",
    "react": "19.1.0",
    "react-native": "^0.81.5",
    "react-native-safe-area-context": "5.6.2",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-web": "^0.21.0",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/js": "^9",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/react": "^16.1.0",
    "@testing-library/react-native": "^13.3.3",
    "@types/react": "~19.1.10",
    "@vitejs/plugin-react": "5.1.3",
    "@vitest/coverage-v8": "1.6.0",
    "eslint": "^9.37.0",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^7.0.1",
    "react-test-renderer": "19.2.4",
    "typescript": "~5.9.2",
    "typescript-eslint": "^8.18.2",
    "vitest": "1.6.1"
  }
}
