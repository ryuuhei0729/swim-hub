{
  "name": "@swim-hub/mobile",
  "version": "1.0.0",
  "private": true,
  "description": "水泳選手マネジメントシステム - モバイルアプリケーション",
  "main": "index.ts",
  "scripts": {
    "start": "npx dotenvx run -f .env.development -- expo start",
    "start:clear": "npx dotenvx run -f .env.development -- expo start --clear",
    "android": "npx dotenvx run -f .env.development -- expo start --android",
    "android:clear": "npx dotenvx run -f .env.development -- expo start --android --clear",
    "ios": "npx dotenvx run -f .env.development -- expo start --ios",
    "ios:run": "npx dotenvx run -f .env.development -- expo run:ios",
    "ios:run:sim": "npx dotenvx run -f .env.development -- expo run:ios --simulator \"iPhone 16\"",
    "ios:clear": "npx dotenvx run -f .env.development -- expo start --ios --clear",
    "web": "npx dotenvx run -f .env.development -- expo start --web",
    "lint": "eslint .",
    "type-check": "tsc --noEmit",
    "clean": "npx expo clean",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "submit:ios": "set -a && source .env && set +a && export APPLE_ID APPLE_APP_SPECIFIC_PASSWORD && eas submit --platform ios --latest"
  },
  "dependencies": {
    "@dotenvx/dotenvx": "^1.51.1",
    "@react-native-async-storage/async-storage": "^2.1.0",
    "@react-native-community/netinfo": "^11.4.1",
    "@react-native-picker/picker": "^2.11.4",
    "@react-navigation/bottom-tabs": "6.5.20",
    "@react-navigation/native": "6.1.18",
    "@react-navigation/native-stack": "6.10.1",
    "@supabase/supabase-js": "^2.80.0",
    "@swim-hub/shared": "*",
    "@tanstack/react-query": "^5.90.10",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "expo": "~54.0.27",
    "expo-clipboard": "8.0.8",
    "expo-dev-client": "^6.0.20",
    "expo-image-picker": "~17.0.10",
    "expo-status-bar": "~3.0.9",
    "react": "19.1.0",
    "react-native": "0.81.5",
    "react-native-safe-area-context": "5.6.0",
    "react-native-screens": "4.16.0",
    "react-native-web": "^0.21.0",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/js": "^9",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/react": "^16.1.0",
    "@testing-library/react-native": "^13.3.3",
    "@types/react": "~19.2.0",
    "@vitejs/plugin-react": "^5.1.2",
    "@vitest/coverage-v8": "^4.0.17",
    "eslint": "^9.37.0",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.1.0",
    "react-test-renderer": "19.1.0",
    "typescript": "~5.9.2",
    "typescript-eslint": "^8.18.2",
    "vitest": "^4.0.17"
  }
}
